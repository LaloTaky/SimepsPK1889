/*Estilos para el bloqueo de las pantallas*/
.LockOff{
	visibility: hidden;
	display: none;
	position: absolute;
	top: -100px;
	left: -100px;
}

.LockBackground{
   position: absolute;
   top: 0px;
   left: 0px;
   visibility: visible;
   display: block;
   width: 105%;
   height: 505%;
   background-color: #666;
   z-index: 999;
   filter: alpha(opacity=55);
   opacity:0.75;
   padding-top: 20%;
}

.LockPane {
	z-index: 1000;
	position: absolute;
	top: 0px;
	left: 0px;
	padding-top: 25%;
	visibility: visible;
	display: block;
	text-align: center;
	width: 100%;
}

.LockPane div {
   color: White;
   width: 55%;	
   font-size: large;
   padding: 9px;
   margin-left: auto;
   margin-right: auto;
   display: block;
}

